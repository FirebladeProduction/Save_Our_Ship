<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
  <ThingDef ParentName="ApparelMakeableBase">
    <defName>Apparel_SpaceJumpsuit_SOS</defName>
    <label>civilian space undersuit</label>
    <description>A tight suit that protects the body from the coldness of space. Its durable and protects against energy based attacks and extreme temperatures, but is otherwise quite unimpressive.</description>
	<recipeMaker>
      <researchPrerequisite>ShipBasics</researchPrerequisite>
	  <skillRequirements>
        <Crafting>8</Crafting>
      </skillRequirements>
    </recipeMaker>
    <graphicData>
      <texPath>Things/Pawn/Humanlike/Apparel/Jumpsuit</texPath>
      <graphicClass>Graphic_Single</graphicClass>
	  <drawSize>1</drawSize>
    </graphicData>
    <costStuffCount>80</costStuffCount>
	<costList>
       <Synthread>20</Synthread>
	   <Chemfuel>25</Chemfuel>
     </costList>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
    <thingCategories>
      <li>Apparel</li>
    </thingCategories>
    <statBases>
	  <MaxHitPoints>150</MaxHitPoints>
	  <WorkToMake>15000</WorkToMake>
      <Mass>0.6</Mass>
      <StuffEffectMultiplierArmor>0.2</StuffEffectMultiplierArmor>
	  <ArmorRating_Heat>0.5</ArmorRating_Heat>
	  <StuffEffectMultiplierInsulation_Cold>1</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>1</StuffEffectMultiplierInsulation_Heat>
      <Insulation_Cold>60</Insulation_Cold>
	  <Insulation_Heat>60</Insulation_Heat>
    </statBases>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Shoulders</li>
		<li>Legs</li>
		<li>Neck</li>
		<li>Arms</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Pawn/Humanlike/Apparel/Jumpsuit</wornGraphicPath>
      <layers>
        <li>OnSkin</li>
      </layers>
      <tags>
        <li>Spacer</li>
		<li>SpacerMilitary</li>
      </tags>
      <defaultOutfitTags>
        <li>Worker</li>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
    <colorGenerator Class="ColorGenerator_StandardApparel" />
  </ThingDef>
  
