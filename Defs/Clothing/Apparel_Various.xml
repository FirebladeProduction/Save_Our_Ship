   <ThingDef ParentName="ArmorMachineableBase">
    <defName>Apparel_Light_EVA_SOS</defName>
    <label>Light EVA Gear</label>
    <description>Offers some protection but its main purpose is to propel the user with the build in thrusters aswell as providing oxygen with its integrated air recycling system.</description>
    <recipeMaker>
      <unfinishedThingDef>UnfinishedMetallicTechArmor</unfinishedThingDef>
      <researchPrerequisite>ShipBasics</researchPrerequisite>
      <skillRequirements>
        <Crafting>8</Crafting>
      </skillRequirements>
	  <recipeUsers Inherit="False">
        <li>FabricationBench</li>
      </recipeUsers>
    </recipeMaker>
    <graphicData>
      <texPath>Things/Pawn/Humanlike/Apparel/LightETMG</texPath>
      <graphicClass>Graphic_Single</graphicClass>
	  <drawSize>1</drawSize>
    </graphicData>
    <stuffCategories>
      <li>Metallic</li>
    </stuffCategories>
    <costStuffCount>50</costStuffCount>
	<costList>
      <Hyperweave>10</Hyperweave>
	  <Plasteel>15</Plasteel>
	  <Uranium>5</Uranium>
	  <ComponentSpacer>1</ComponentSpacer>
    </costList>
    <statBases>
      <WorkToMake>27000</WorkToMake>
      <MaxHitPoints>300</MaxHitPoints>
      <Mass>5</Mass>
      <Flammability>0.6</Flammability>
	  <StuffEffectMultiplierArmor>0.5</StuffEffectMultiplierArmor>
      <EquipDelay>5</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <MoveSpeed>0.5</MoveSpeed>
    </equippedStatOffsets>
    <thingCategories>
      <li>Apparel</li>
    </thingCategories>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
        <li>Shoulders</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Pawn/Humanlike/Apparel/LightETMG</wornGraphicPath>
      <layers>
        <li>Middle</li>
      </layers>
      <tags>
        <li>Spacer</li>
		<li>SpacerMilitary</li>
		<li>EVA_GasTank</li>
      </tags>
      <defaultOutfitTags>
        <li>Worker</li>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <weight>10</weight>
          <only>(0.33,0.33,0.33)</only>
        </li>
        <li>
          <weight>10</weight>
          <only>(0.65,0.65,0.65)</only>
        </li>
        <li>
          <weight>6</weight>
          <min>(0.3,0.3,0.3)</min>
          <max>(0.5,0.5,0.5)</max>
        </li>
      </options>
    </colorGenerator>
  </ThingDef>
  
