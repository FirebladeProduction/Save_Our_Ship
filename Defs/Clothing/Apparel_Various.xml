  <ThingDef ParentName="ArmorHelmetMakeableBase">
    <defName>Apparel_Light_Spacehelmet_SOS</defName>
    <label>Light EVA helmet</label>
    <description>A helmet which protects against the vacuum of space, it doesnt offer much protection. Its inbuild communications device allows clear conversations and the build in translator makes communication easier too.</description>
    <techLevel>Spacer</techLevel>
    <graphicData>
      <texPath>Things/Pawn/Humanlike/Apparel/LightSpacehelmet</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <stuffCategories>
      <li>Metallic</li>
    </stuffCategories>
		<costStuffCount>40</costStuffCount>
		<statBases>
			<WorkToMake>3200</WorkToMake>
			<MaxHitPoints>100</MaxHitPoints>
			<Mass>2</Mass>
      <StuffEffectMultiplierArmor>0.5</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.15</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>2.5</EquipDelay>
    </statBases>
	<equippedStatOffsets>
      <SocialImpact>0.05</SocialImpact>
    </equippedStatOffsets>
	<costList>
      <ComponentSpacer>2</ComponentSpacer>
      <Plasteel>10</Plasteel>
    </costList>
    <recipeMaker>
	  <unfinishedThingDef>UnfinishedMetallicTechArmor</unfinishedThingDef>
	  <researchPrerequisite>ShipBasics</researchPrerequisite>
      <recipeUsers Inherit="False">
        <li>FabricationBench</li>
      </recipeUsers>
      <skillRequirements>
        <Crafting>8</Crafting>
      </skillRequirements>
    </recipeMaker>
		<apparel>
      <bodyPartGroups>
        <li>FullHead</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Pawn/Humanlike/Apparel/LightSpacehelmet</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
        <li>Spacer</li>
		<li>SpacerMilitary</li>
		<li>EVA_Helmet</li>
      </tags>
      <defaultOutfitTags>
		<li>Worker</li>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
  </ThingDef>
  
